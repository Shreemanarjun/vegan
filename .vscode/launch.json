    "version": "0.2.0",
    "configurations": [
        {
            "name": "vegan(no debug mode)",
            "request": "launch",
            "type": "dart",
            "noDebug": true,
            // "args": [
            //     "--route",
            //     "/dashboard_page/coming_soon_tab/"
            // ],
        },
        {
            "name": "vegan(debug mode)",
            "request": "launch",
            "type": "dart",
        },
        {
            "name": "vegan (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "vegan (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Flutter for web (hot reloadable)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-experimental-hot-reload",
            ]
        }
    ]
}